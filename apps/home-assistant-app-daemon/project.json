{
  "name": "home-assistant-app-daemon",
  "sourceRoot": "{projectRoot}/src",
  "projectType": "application",
  "targets": {
    "package": {
      "executor": "@ha/nx-executors:invoke",
      "options": {
        "cwd": "{projectRoot}",
        "module": "scripts/package.ts"
      }
    },
    "publish": {
      "executor": "@ha/nx-executors:invoke",
      "options": {
        "cwd": "{projectRoot}",
        "module": "scripts/publish.ts"
      }
    }
  },
  "tags": [],
  "implicitDependencies": []
}
