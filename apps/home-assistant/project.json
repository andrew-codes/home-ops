{
  "name": "home-assistant",
  "sourceRoot": "{projectRoot}/src",
  "projectType": "application",
  "targets": {
    "deploy": {
      "executor": "@ha/nx-executors:invoke",
      "options": {
        "cwd": "{projectRoot}",
        "module": "scripts/deploy.ts"
      }
    }
  },
  "tags": [],
  "implicitDependencies": [
    "cloudflared",
    "home-assistant-app-daemon",
    "metrics",
    "mqtt",
    "nfs-provisioner",
    "pihole-service",
    "playnite-web-app",
    "ps5",
    "zwavejs"
  ]
}
